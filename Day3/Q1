#include<stdio.h>
void main()
{
    for (int i = 5, x = 0; i >= 1; i--, x += 2) 
    {
            for (int j = 1; j <= i; j++) {
                printf("%d",j);
            }
            for (int y = 1; y <= x; y++) {
                printf(" ");
            }
            for (int k = i; k >= 1; k--) {
                printf("%d", k);
            }
            printf("\n");
        }
        return 0;

}